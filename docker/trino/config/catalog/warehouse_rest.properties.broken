connector.name=iceberg
iceberg.catalog.type=REST
iceberg.rest-catalog.uri=http://iceberg-rest:8181
iceberg.rest-catalog.warehouse=s3://warehouse/

fs.native-s3.enabled=true
s3.endpoint=http://minio:9000
s3.region=us-east-1
s3.path-style-access=true
s3.aws-access-key=${ENV:MINIO_ROOT_USER}
s3.aws-secret-key=${ENV:MINIO_ROOT_PASSWORD}
s3.max-connections=500
s3.streaming.part-size=64MB

iceberg.file-format=PARQUET
iceberg.compression-codec=GZIP
iceberg.target-max-file-size=256MB
iceberg.extended-statistics.collect-on-write=true
iceberg.dynamic-filtering.wait-timeout=5m

parquet.writer.validation-percentage=0.0
parquet.writer.block-size=134217728B
parquet.writer.page-size=1048576B